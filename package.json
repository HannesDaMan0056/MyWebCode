{
  "name": "skillbased-homepage",
  "version": "0.0.1",
  "private": true,
  "description": "landing page",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir src --write src",
    "start-old": "http-server -p 8005 ./Definity_MultipurposeOne-MultiPageTemplate/Definity -o /index.html",
    "start": "cd Definity_MultipurposeOne-MultiPageTemplate/Definity && live-server --port=8006",
    "update": "npx npm-check-updates -u && npm install && npm audit fix"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/adapter-static": "^2.0.2",
    "@sveltejs/kit": "^1.5.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte": "^2.26.0",
    "flowbite-typography": "^1.0.3",
    "postcss": "^8.4.14",
    "postcss-load-config": "^4.0.1",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.8.1",
    "rollup-plugin-copy": "^3.4.0",
    "svelte": "^3.54.0",
    "svelte-check": "^3.0.1",
    "svelte-image": "^0.2.9",
    "svelte-preprocess": "^5.0.3",
    "tailwindcss": "^3.1.5",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^4.3.0",
    "vite-imagetools": "^4.0.19"
  },
  "type": "module",
  "dependencies": {
    "@popperjs/core": "^2.11.7",
    "classnames": "^2.3.2",
    "flowbite": "^1.6.5",
    "flowbite-svelte": "^0.34.9",
    "http-server": "^14.1.0",
    "live-server": "^1.2.2",
    "watch-http-server": "^0.7.6"
  }
}
