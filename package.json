{
	"name": "skillbased-homepage",
	"version": "0.0.1",
	"private": true,
	"description": "landing page",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"start-old": "http-server -p 8005 ./Definity_MultipurposeOne-MultiPageTemplate/Definity -o /index.html",
		"start": "cd Definity_MultipurposeOne-MultiPageTemplate/Definity && live-server --port=8006",
		"update": "npx npm-check-updates -u && npm install && npm audit fix"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.5.0",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.26.0",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.3.0"
	},
	"type": "module",
	"dependencies": {
		"flowbite": "^1.6.5",
		"http-server": "^14.1.0",
		"live-server": "^1.2.2",
		"watch-http-server": "^0.7.6"
	}
}
